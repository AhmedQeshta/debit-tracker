{
  "expo": {
    "name": "Debit Tracker",
    "slug": "debit-tracker",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "debit-tracker",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#121829"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ahmedqeshta.debittracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#121829"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ahmedqeshta.debittracker",
      "jsEngine": "hermes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "6362e19c-3fa4-4d07-bf30-3f65708feea9"
      },
      "router": {}
    },
    "owner": "ahmedqeshta2",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/6362e19c-3fa4-4d07-bf30-3f65708feea9"
    }
  }
}
